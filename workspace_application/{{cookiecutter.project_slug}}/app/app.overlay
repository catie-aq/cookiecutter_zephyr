#include <zephyr/dt-bindings/gpio/sixtron-header.h>

{% if cookiecutter.secure_app|lower == "true" -%}
/ {
	zephyr,user {
		stsafereset-gpios = <&sixtron_connector DIO1 GPIO_ACTIVE_HIGH>; // R3
	};
};

&sixtron_i2c {
	status = "okay";
	clock-frequency = <I2C_BITRATE_STANDARD>;
};
{% endif -%}
